{
  "extends": [
    "github>grafbase/renovate",
    "github>grafbase/renovate:post-upgrade"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["axum"],
      "matchFileNames": ["crates/graphql-mocks/Cargo.toml"],
      "matchUpdateTypes": ["patch"]
    },
    {
      "matchPackageNames": [
        "async-tungstenite",
        "ory-client",
        "grafbase-sdk",
        "tonic"
      ],
      "enabled": false
    },
    {
      "matchPackageNames": ["rand"],
      "matchFileNames": ["crates/integration-tests/Cargo.toml"],
      "enabled": false
    }
  ]
}
